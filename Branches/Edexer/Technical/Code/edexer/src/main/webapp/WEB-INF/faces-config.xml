<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
	<navigation-rule>
		<from-view-id>/register.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/register.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>staff</from-outcome>
			<to-view-id>/staff/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>user</from-outcome>
			<to-view-id>/user/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/register.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>staff</from-outcome>
			<to-view-id>/staff/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>user</from-outcome>
			<to-view-id>/user/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/user/addbusinesscard.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>newcard</from-outcome>
			<to-view-id>/user/addbusinesscard.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/user/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>user</from-outcome>
			<to-view-id>/user/user_home.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>actionlogout</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/user/usersettings.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>saved</from-outcome>
			<to-view-id>/user/usersettings.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>actionlogout</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/templates/common/commonHeader.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>corp_home</from-outcome>
			<to-view-id>../user/corp/corp_user_home.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>corp_upg</from-outcome>
			<to-view-id>../user/corp_upgrade.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>website</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>IndexRule</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/user/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>user</from-outcome>
			<to-view-id>/user/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>Admin</from-outcome>
			<to-view-id>/staff/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>User</from-outcome>
			<to-view-id>/user/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/user/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionlogout</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/addsubscription.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionlogout</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/user/management.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionlogout</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/user/businesscard_details.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionlogout</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/user/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>bc_details</from-outcome>
			<to-view-id>/user/businesscard_details.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>unauthorized</from-outcome>
			<to-view-id>/unauthorized.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/user/personal/pro_upgrade.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/user/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/user/corp/corp_upgrade.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/user/corp/management.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/staff/adduser.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/staff/users.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/staff/adduser.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/passwordreset.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<application>
		<locale-config>
			<default-locale>en</default-locale>
		</locale-config>
		<resource-bundle>
			<base-name>com.lang.default</base-name>
			<var>content</var>
		</resource-bundle>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>
</faces-config>
